<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	{foreach $entries as $entry}
		<url>
			<loc>{$entry->loc}</loc>
			<lastmod n:if="$entry->lastmod">{$entry->lastmod}</lastmod>
			<changefreq n:if="$entry->changefreq">{$entry->changefreq}</changefreq>
			<priority n:if="$entry->priority">{$entry->priority}</priority>
		</url>
	{/foreach}
</urlset>